<UserControl x:Class="PDF_Merger.Controls.SingleTab"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PDF_Merger.Controls" xmlns:PDFViewer="clr-namespace:Syncfusion.Windows.PdfViewer;assembly=Syncfusion.PdfViewer.WPF"
             mc:Ignorable="d" 
             Background="White"
             d:DesignHeight="700" d:DesignWidth="1000">
    <Grid>
        <PDFViewer:PdfViewerControl x:Name="pdfViewer"
                                    DataContextChanged="PdfViewer_DataContextChanged"/>
    </Grid>
</UserControl>

<Page
xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
xmlns:viewmodels="clr-namespace:PDF_Merger.ViewModels"
xmlns:syncfusion="http://schemas.syncfusion.com/wpf" 
xmlns:controls="clr-namespace:PDF_Merger.Controls"
xmlns:vm="clr-namespace:PDF_Merger.ViewModels"
x:Class="PDF_Merger.Views.PDFViewer"
mc:Ignorable="d"
d:DesignHeight="800" d:DesignWidth="1000"
Title="PDFViewer"
Background="White"
d:DataContext="{d:DesignInstance Type={x:Type viewmodels:PDFViewModel}}">

    <DockPanel>
        <syncfusion:TabControlExt x:Name="TabControl" 
                                  SelectedIndex="{Binding SelectedIndex}"
                                  IsNewButtonEnabled="True"
                                  NewButtonAlignment="Last"
                                  NewButtonClick="TabControl_NewButtonClick"
                                  />


    </DockPanel>

</Page>
